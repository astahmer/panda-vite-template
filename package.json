{
  "name": "panda-vite-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "panda codegen",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "dev:css": "pnpm css:gen -w",
    "css:gen": "panda cssgen --lightningcss",
    "park-ui:up": "bunx @park-ui/cli add --all",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ark-ui/react": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.32.0",
    "@park-ui/panda-preset": "^0.35.2",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "lightningcss": "^1.24.0",
    "postcss": "^8.4.35",
    "typescript": "^5.3.3",
    "vite": "^5.1.4"
  }
}
